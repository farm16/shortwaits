    "test": "nx test",
    "clean": "sh ./scripts/clean.sh",
    "all": "nx run-many --target=serve --projects=sw-mobile-admin,sw-api,sw-web --parallel=3",
    "generate-keypair": "ts-node ./src/scripts/create-key-pair",
    "prepare": "husky install",
    "push:main": "git push --follow-tags origin main"
