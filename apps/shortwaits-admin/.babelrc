{
  "presets": ["module:metro-react-native-babel-preset"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [".", "./src"],
        "extensions": [".js", ".json"],
        "alias": {
          "@shortwaits/admin": "./src"
        }
      }
    ],
    "react-native-reanimated/plugin"
  ],
  "env": {
    "production": {
      "plugins": ["react-native-paper/babel"]
    }
  }
}
